var $doc          = jQuery(document),
	$body         = jQuery('body');
	
$doc.ready(function(){
var $onTheFlyChange = jQuery( '.change-on-the-fly' );
	$onTheFlyChange.click(function(e){
		var thisLink = jQuery(this);
		if(thisLink.hasClass( 'magazine-layout-1' )){
			$body.addClass( 'magazine1' ).removeClass( 'magazine2 magazine3' );
		}else if(thisLink.hasClass( 'magazine-layout-2' )){
			$body.addClass( 'magazine2' ).removeClass( 'magazine1 magazine3' );
		}else if(thisLink.hasClass( 'magazine-layout-3' )){
			$body.addClass( 'magazine3' ).removeClass( 'magazine1 magazine2' );
		}
		// e.preventDefault();
		return false;
	});
});
